{
    "name": "malpaso/laravel-tmdb",
    "description": "Laravel wrapper for The Movie Database (TMDB) API v3",
    "version": "1.0.0",
    "keywords": [
        "laravel",
        "tmdb",
        "movies",
        "tv shows",
        "api",
        "wrapper"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/cache": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "pestphp/pest": "^2.0|^3.0",
        "pestphp/pest-plugin-laravel": "^2.0|^3.0",
        "mockery/mockery": "^1.4",
        "phpstan/phpstan": "^1.0|^2.0",
        "rector/rector": "^1.0",
        "vlucas/phpdotenv": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "malpaso\\LaravelTmdb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "malpaso\\LaravelTmdb\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "pest",
        "test-unit": "pest tests/Unit tests/Feature --exclude-group=integration",
        "test-integration": "pest tests/Integration --group=integration",
        "test-coverage": "pest --coverage",
        "analyse": "phpstan analyse",
        "refactor": "rector process",
        "refactor-dry": "rector process --dry-run"
    },
    "extra": {
        "laravel": {
            "providers": [
                "malpaso\\LaravelTmdb\\TmdbServiceProvider"
            ],
            "aliases": {
                "Tmdb": "malpaso\\LaravelTmdb\\Facades\\Tmdb"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
